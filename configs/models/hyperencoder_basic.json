{
    "model_type": "hyperencoder",
    "model": {
        "encoder": {
            "type": "oobleck",
            "config": {
                "in_channels": 64,
                "channels": 4,
                "latent_dim": 4,
                "c_mults": [16, 8, 4, 2, 2],
                "strides": [8, 8, 4, 4, 1],
                "use_snake": false
            }
        },
        "decoder": {
            "type": "oobleck",
            "config": {
                "out_channels": 64,
                "channels": 4,
                "latent_dim": 4,
                "c_mults": [16, 8, 4, 2, 2],
                "strides": [8, 8, 4, 4, 1],
                "use_snake": false,
                "final_tanh": false
            }
        },
        "bottleneck": {
            "type": "vae"
        },
        "latent_dim": 4,
        "in_channels": 64,
        "out_channels": 64
    },
    "training": {
        "optimizer_configs": {
            "hyperencoder": {
                "optimizer": {
                    "type": "AdamW",
                    "config": {
                        "lr": 5e-5,
                        "betas": [0.9, 0.999],
                        "weight_decay": 1e-3
                    }
                },
                "scheduler": {
                    "type": "InverseLR",
                    "config": {
                        "inv_gamma": 1000000,
                        "power": 0.5,
                        "warmup": 0.99
                    }
                }
            }
        }
    },
    "demo": {
        "demo_every": 20,
        "max_demos": 10
    }
}
{
    "model_type": "hyperencoder",
    "model": {
        "encoder": {
            "type": "oobleck",
            "config": {
                "in_channels": 64,
                "channels": 8,
                "latent_dim": 8,
                "c_mults": [1, 2, 4, 8, 16],
                "strides": [2, 4, 4, 4, 8],
                "use_snake": true
            }
        },
        "decoder": {
            "type": "oobleck",
            "config": {
                "out_channels": 64,
                "channels": 8,
                "latent_dim": 4,
                "c_mults": [1, 2, 4, 8, 16],
                "strides": [2, 4, 4, 4, 8],
                "use_snake": true,
                "final_tanh": false
            }
        },
        "bottleneck": {
            "type": "vae"
        },
        "latent_dim": 4,
        "in_channels": 64,
        "out_channels": 64
    },
    "training": {
        "optimizer_configs": {
            "hyperencoder": {
                "optimizer": {
                    "type": "AdamW",
                    "config": {
                        "lr": 5e-5,
                        "betas": [0.9, 0.999],
                        "weight_decay": 1e-3
                    }
                }
            }
        }
    },
    "demo": {
        "demo_every": 1024,
        "max_demos": 16
    }
}